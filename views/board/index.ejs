<%- include(VIEWS+'/common/head') %>

<!-- Breadcrumbs-->
<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a href="/board">게시판</a>
    </li>
</ol>

<div class="card boardWrap listWrap" data-write-popup-url="/board/write">
	<div class="card-body">
		<div class="table-responsive">
			<div class="form-inline">
				<div class="input-group">
					<span class="input-group-addon" id="basic-addon1">기간검색</span>
					<input type="text" class="form-control date-picker sch_start_date" value="<%=sch_start_date%>" placeholder="YYYY-MM-DD" />
					<span class="input-group-addon">~</span>
					<input type="text" class="form-control date-picker sch_end_date" value="<%=sch_end_date%>" placeholder="YYYY-MM-DD" />
				</div>
			</div>
		</div>
	</div>
	<div class="card-body">
		<div class="table-responsive">
			<table class="table table-bordered bulk_action publicDataTable" width="100%" cellspacing="0" data-base-uri="/board">
				<colgroup>
					<col width="8%;" />
					<col width="*;" />
					<col width="15%;" />
					<col width="10%;" />
					<col width="10%;" />
				</colgroup>
				<thead>
				<tr>
					<th class="text-center">번호</th>
					<th class="text-center">제목</th>
					<th class="text-center">작성자</th>
					<th class="text-center">등록일</th>
					<th class="text-center">조회수</th>
				</tr>
				</thead>
				<tbody></tbody>
			</table>
		</div>
	</div>
	<div class="card-body">
		<a href="javascript:void(0);" class="btn btn-block btn-primary writePopupBtn">등록</a>
	</div>
</div>

<%- include(VIEWS+'/common/footer') %>